#include<iostream>
using namespace std;
int main()
{
   for(int i=5;i>0;i--)
   {
       for(int j=6;j>1;j--)
       {
           if(j>i)
           {
               cout<<j-1<<" ";
           }
           else
           {
               cout<<i<<" ";
           }
        }
        cout<<"\n";
   }
}
